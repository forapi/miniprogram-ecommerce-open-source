#login-type{padding:0 15px}#login-type .phone__warning{text-align:center;width:95px;height:95px;margin:59px auto 129px;overflow:hidden}#login-type .phone__warning image{width:100%;height:100%}#login-type .phone__warning i{color:#FFBE00;font-size:50px}#login-type .phone__text{font-size:14px;color:#888;line-height:20px;margin-bottom:32px}#login-type .phone__btn{width:100%;height:50px;line-height:50px;font-size:16px;display:flex;align-items:center;justify-content:center;border-radius:4px}#login-type .phone__btn.wechat-phoneBtn{color:#FFF;background:#ff2741;margin-bottom:26px;border:1px solid #ff2741}#login-type .phone__btn.again-phoneBtn{color:#ff2741;background:#fff;border:1px solid #ff2741}#login-type .phone__btn:after{border:none}#login-type .phone__btn i{padding-right:10px}#login-type .phone_login{text-align:center;font-size:15px;color:#999}#login-type .getUser{opacity:0;position:absolute;top:0;bottom:0;left:0;right:0;padding:0}#login-type .select{position:absolute;bottom:40px;text-align:center;color:#999;font-size:12px;left:15px;right:15px}#login-type .greement-box{position:fixed;width:85%;top:50%;left:50%;transform:translate(-50%,-50%);background:#FFF;border-radius:5px;overflow:hidden;text-align:center;z-index:300}#login-type .greement-box .title{font-weight:700;padding:15px 0}#login-type .greement-box .content{text-align:center;padding:15px;max-height:250px;overflow:auto}#login-type .greement-box .btn{padding:15px;color:#ff2741}#login-type .mask{position:fixed;top:0;left:0;right:0;bottom:0;z-index:200;background-color:rgba(0,0,0,.4)}